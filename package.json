{
	"name": "vocawiki-frontend",
	"scripts": {
		"format": "prettier \"*.{ts,css,scss,json,md}\" \"src/**/*.{ts,css,scss,json,md}\" \"scripts/**/*.{ts,css,scss,json,md}\" --write --log-level warn",
		"check:type": "bun run ./scripts/check-type",
		"check:lint": "eslint .",
		"check": "bun run check:type && bun run check:lint",
		"build": "bun run ./scripts/build",
		"deploy": "bun run ./scripts/deploy",
		"before-commit": "bun run format && bun run check && bun run build"
	},
	"private": true,
	"type": "module",
	"devDependencies": {
		"@eslint/js": "^9.36.0",
		"@ianvs/prettier-plugin-sort-imports": "^4.7.0",
		"@types/fs-extra": "^11.0.4",
		"bun": "^1.2.22",
		"bun-types": "^1.2.22",
		"eslint": "^9.36.0",
		"eslint-plugin-react": "^7.37.5",
		"fs-extra": "^11.3.2",
		"globals": "^16.4.0",
		"prettier": "^3.6.2",
		"radashi": "^12.6.2",
		"rolldown": "1.0.0-beta.43",
		"sass-embedded": "^1.93.2",
		"type-fest": "^5.0.1",
		"types-mediawiki": "^2.0.0",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.44.1",
		"wiki-saikou": "^5.0.0"
	},
	"packageManager": "pnpm@10.17.1+sha512.17c560fca4867ae9473a3899ad84a88334914f379be46d455cbf92e5cf4b39d34985d452d2583baf19967fa76cb5c17bc9e245529d0b98745721aa7200ecaf7a",
	"dependencies": {
		"fast-average-color": "^9.5.0"
	}
}
